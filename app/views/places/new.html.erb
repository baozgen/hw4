<div class="row mt-3 mb-3">
  <div class="col">
    <h1>New Place</h1>
  </div>
</div>

<%= form_with :model => @place do |form| %>
  <div class="mb-3">
    <%= form.label "name", class: "form-label"  %>
    <%= form.text_field "name", class: "form-control"  %>
  </div>

    <%= form.submit "Create Place",class: "btn btn-primary"%>
<% end %>