<div class="row mt-3 mb-3">
  <div class="col">
    <h1>New Post</h1>
  </div>
</div>

<%= form_with :model => @post do |form| %>
  <%= form.hidden_field "place_id" %>
  <p>
    <%= form.label "title" %>
    <%= form.text_field "title" %>
  </p>
  <p>
    <%= form.label "description" %>
    <%= form.text_field "description" %>
  </p>
  <p>
    <%= form.label "posted_on" %>
    <%= form.date_field "posted_on" %>
  </p>
  <p>
    <%= form.submit %>
  </p>
<% end %>
